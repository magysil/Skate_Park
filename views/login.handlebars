 <h1>{{>partials/title}}</h1>

    <div class="py-5">
      <h2>{{>partials/titleTabla}}</h2>
      <hr/>

      
       <form id="loginForm">
        <div class="form-group">
          <div class="form-group">
            <label for="email">Email</label>
            <input type="email" id="email" name="email" class="form-control w-50 m-auto" required />
          </div>
          <div class="form-group mt-3">
            <label for="password">Password</label>
            <input type="password" id="password" name="password" class="form-control w-50 m-auto" required />
          </div>
        </div>
        <button type="submit" class="btn btn-success my-3">Ingresar</button>
        <p>¿Aún no tienes cuenta? <a href="/registro">Regístrate</a></p>
      </form>
    </div>