    <div class="my-5 py-4">
      <h2>{{>partials/titleTabla}}</h2>

      <hr />

  <form id="editarForm">
    <div class="form-group row w-50 m-auto">
      <div class="form-group col-12 col-sm-6">
        <label>Email</label>
        <input
          class="form-control m-auto"
          value="{{skater.email}}"
          disabled
        />
      </div>
      <div class="form-group col-12 col-sm-6">
        <label>Nombre</label>
        <input class="form-control m-auto" id="nombre" value="{{skater.nombre}}" />
      </div>
      <div class="form-group col-12 col-sm-6">
        <label>Password</label>
        <input
          type="password"
          class="form-control m-auto"
          id="password"
          value="{{skater.password}}"
        />
      </div>
      <div class="form-group col-12 col-sm-6">
        <label>Repita la password</label>
        <input
          type="password"
          class="form-control m-auto"
          id="confirmPassword"
          value="{{skater.password}}"
        />
      </div>
      <div class="form-group col-12 col-sm-6">
        <label>AÃ±os de experiencia</label>
        <input class="form-control m-auto" id="anos_experiencia" value="{{skater.anos_experiencia}}" />
      </div>
      <div class="form-group col-12 col-sm-6">
        <label>Especialidad</label>
        <input class="form-control m-auto" id="especialidad" value="{{skater.especialidad}}" />
      </div>
    </div>
    <div class="my-1">
      <button type="button" class="btn btn-primary" id="botonActualizar">Actualizar</button>
    </div>
    <div>
      <button type="button" class="btn btn-danger" id="botonEliminar">Eliminar cuenta</button>
    </div>
  </form>
</div>
